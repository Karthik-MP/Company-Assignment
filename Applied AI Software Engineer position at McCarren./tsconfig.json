{
  "compilerOptions": {
    "target": "ES2019",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "lib": ["DOM", "ES2019"],
    "types": ["office-js"],
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["src/**/*.ts", "vite.config.ts"],
  "exclude": ["node_modules"]
}
